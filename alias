
alias cath="cat /etc/hosts"
alias vih="sudo vim /etc/hosts"
alias catu="cat ~/dotfiles/upgrade.lst"
alias viupg="vim ~/dotfiles/upgrade.lst"
alias catr="grep linux_build /etc/calculate/calculate.env"
alias catro="grep linux_build /mnt/etc/calculate/calculate.env"
alias catb="l /var/calculate/backup/cldx-*"
alias catns="cat ~/.oracle/tnsnames.ora"
alias vicfg="vim ~/.vimrc"
alias catm="cat /etc/portage/make.conf/custom"
alias vimk="sudo vim /etc/portage/make.conf/custom" 
alias vitns="vim ~/.oracle/tnsnames.ora" 
alias catw="cat /var/lib/portage/world"
alias naf="sudo pppd call nafta"
alias df="df -h"
alias slog="sudo tail -100 /var/log/messages"
alias ping="sudo ping -c3"
alias pya="sudo ping -c3 ya.ru"
alias pingz="sudo ping -c3 zurodov.no-ip.org"
alias dualmon="xrandr --output HDMI2 --auto --left-of HDMI1 --output HDMI1
--mode 1920x1080 --brightness 0.5 --pos 1200x200"


